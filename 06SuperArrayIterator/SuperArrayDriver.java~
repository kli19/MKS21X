public class SuperArrayDriver{

      public static void main(String[] args){
	  SuperArray x = new SuperArray();
	  x.add("y");
	  x.add("1");
	  x.add("2");
	  x.add("3");
	  x.add("4");
	  x.add("5");
	  x.add("y");
	  System.out.println(x.toString());
	
	  System.out.println(x.contains("1"));
	   System.out.println(x.contains("10"));

	   System.out.println(x.indexOf("y"));
	  System.out.println(x.size());
	  System.out.println(x.get(-1));
	  System.out.println(x.lastIndexOf("y"));
	  x.add(5, "insert");
	  System.out.println(x.toString());

	  System.out.println(x.remove(0));
	  System.out.println(x.toString());
	  System.out.println(x.indexOf("y"));
	  x.remove("y");
	  System.out.println(x.toString());
	  if (x.remove("y"))System.out.println("true");
      }

    
       
}
